{
  "name": "image-builder",
  "version": "1.1.0",
  "private": false,
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "dependencies": {
    "@babel/runtime": "7.15.4",
    "@data-driven-forms/pf4-component-mapper": "3.12.2",
    "@data-driven-forms/react-form-renderer": "3.12.1",
    "@patternfly/patternfly": "4.135.2",
    "@patternfly/react-core": "4.157.3",
    "@patternfly/react-table": "4.30.3",
    "@redhat-cloud-services/frontend-components": "3.4.6",
    "@redhat-cloud-services/frontend-components-notifications": "3.2.4",
    "@redhat-cloud-services/frontend-components-utilities": "3.2.5",
    "classnames": "2.3.1",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-redux": "7.2.5",
    "react-router-dom": "5.3.0",
    "redux": "4.1.1",
    "redux-logger": "3.0.6",
    "redux-promise-middleware": "6.1.2",
    "redux-thunk": "2.3.0"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/stories/*",
      "!src/entry-dev.js"
    ],
    "globals": {
      "COMMITHASH": "dummy"
    },
    "roots": [
      "<rootDir>/src/"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    }
  },
  "devDependencies": {
    "@babel/core": "7.8.4",
    "@babel/plugin-proposal-object-rest-spread": "7.15.6",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-runtime": "7.8.3",
    "@babel/preset-env": "7.8.7",
    "@babel/preset-flow": "7.14.5",
    "@babel/preset-react": "7.14.5",
    "@redhat-cloud-services/frontend-components-config": "4.3.9",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "11.0.4",
    "@testing-library/user-event": "13.3.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.3",
    "babel-jest": "26.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-dual-import": "1.2.1",
    "babel-plugin-lodash": "3.3.4",
    "css-loader": "5.2.4",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-loader": "3.0.3",
    "eslint-plugin-jest-dom": "3.2.4",
    "eslint-plugin-react": "7.18.3",
    "eslint-plugin-testing-library": "3.10.0",
    "git-revision-webpack-plugin": "5.0.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.1.0",
    "npm-run-all": "4.1.5",
    "prop-types": "15.7.2",
    "redux-mock-store": "1.5.4",
    "sass": "1.35.2",
    "sass-loader": "^12.2.0",
    "stylelint": "13.1.0",
    "stylelint-config-recommended-scss": "4.3.0",
    "stylelint-scss": "3.14.2",
    "webpack-bundle-analyzer": "4.4.2"
  },
  "scripts": {
    "build": "webpack --config config/prod.webpack.config.js",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint config src",
    "lint:js:fix": "eslint config src --fix",
    "lint:sass": "stylelint 'src/**/*.scss' --config .stylelintrc.json",
    "nightly": "npm run travis:verify",
    "prod": "NODE_ENV=production BETA=true webpack serve --config config/dev.webpack.config.js",
    "server:ctr": "node src/server/generateServerKey.js",
    "start": "NODE_ENV=development webpack serve --config config/dev.webpack.config.js",
    "start:beta": "NODE_ENV=development BETA=true webpack serve --config config/dev.webpack.config.js",
    "start:proxy": "PROXY=true npm run start",
    "start:proxy:beta": "BETA=true npm run start:proxy",
    "test": "jest --verbose",
    "test:single": "jest --verbose -w 1",
    "travis:build": "NODE_ENV=production webpack --config config/prod.webpack.config.js",
    "travis:verify": "npm-run-all travis:build lint test",
    "verify": "npm-run-all build lint test"
  },
  "insights": {
    "appname": "image-builder"
  }
}
